extends layout

block content
  h3.muted Stats

  table.table.table-striped(style="table-layout: fixed; width: 50%;")
    thead
      tr
        th Day
        th.r Home
        th.r Shorten
        th.r Expand
    tbody
      for ts in Object.keys(stats).sort()
        tr
          td= (new Date(ts * 1000)).toISOString()
          td.r= stats[ts].home || 0
          td.r= stats[ts].shorten || 0
          td.r= stats[ts].expand || 0

  p (Ends)
